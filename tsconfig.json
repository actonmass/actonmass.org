{
  "compilerOptions": {
    "noImplicitAny": false,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "./.ts-dist/",
    "incremental": true,
    "moduleResolution": "node",
    "strictNullChecks": true
  },
  "exclude": ["node_modules/**"]
}
