{
  "name": "actonmass",
  "scripts": {
    "build": "webpack --mode=production && ./scripts/build-cms-config.sh && bundle exec jekyll build",
    "serve": "webpack --mode=development && ./scripts/build-cms-config.sh && bundle exec jekyll serve",
    "watch-js": "webpack --watch --mode=development",
    "netlify-cms": "netlify-cms-proxy-server",
    "dev": "env `cat .env` netlify dev --port 4000",
    "ts-watch": "tsc -b --watch",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.10.1",
    "@types/axios": "^0.14.0",
    "@types/dot": "^1.1.4",
    "@types/jest": "^26.0.3",
    "@types/lodash": "^4.14.157",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-modal": "^3.10.5",
    "babel-loader": "^8.0.6",
    "dotenv": "^8.2.0",
    "jest": "^26.1.0",
    "js-yaml": "^4.0.0",
    "netlify-cli": "^2.21.0",
    "netlify-cms-proxy-server": "^1.3.11",
    "netlify-lambda": "^1.6.3",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "dot": "^1.1.3",
    "leaflet": "^1.6.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-leaflet": "^2.6.0",
    "react-markdown": "^4.3.1",
    "react-modal": "^3.11.2",
    "semantic-ui-react": "^2.0.3"
  }
}
